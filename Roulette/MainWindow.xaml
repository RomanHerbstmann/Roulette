<!--Region Window initial-->
<Window x:Class="Roulette.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Roulette"
        xmlns:controls="clr-namespace:Roulette"
        mc:Ignorable="d"
        Title="MainWindow" 
        Height="1080" 
        Width="1920"
        Background="#2d2d2d"
        WindowStartupLocation="CenterScreen"    
        WindowStyle="None"
        WindowState="Maximized">
    <!--Endregion-->
    <Window.DataContext>
        <local:ViewModel/>
    </Window.DataContext>
    <Page xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" WindowTitle="ZIndex Sample">
        <Grid>
            <!--Region RowDefinition-->
            <Grid.RowDefinitions>
                <RowDefinition Height="5*"/>
                <RowDefinition Height="12*"/>
                <RowDefinition Height="63*"/>
                <RowDefinition Height="20*"/>
            </Grid.RowDefinitions>
            <!--Endregion-->
            <!-- Region FAQ-->
            <StackPanel Grid.RowSpan="4" Width="1920" Height="1080" Background="#2d2d2d" Opacity="0.9" VerticalAlignment="Center" HorizontalAlignment="Center" x:Name="FaqPanel">
                <StackPanel.Style>
                    <Style TargetType="StackPanel">
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding ShowFaq}" Value="false">
                                <Setter Property="Grid.ZIndex" Value="0"></Setter>
                                <Setter Property="Visibility" Value="Hidden"></Setter>
                            </DataTrigger>
                            <DataTrigger Binding="{Binding ShowFaq}" Value="true">
                                <Setter Property="Grid.ZIndex" Value="1"></Setter>
                                <Setter Property="Visibility" Value="Visible"></Setter>
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </StackPanel.Style>
                <Border HorizontalAlignment="Right" BorderBrush="White" BorderThickness="1" CornerRadius="5" Margin="5">
                    <Grid>
                        <Button Grid.ZIndex="2" Background="Transparent"  Command="{Binding btnHideFAQClickCommand}"></Button>
                        <Rectangle Width="10" Height="100" Stroke="White" StrokeThickness="10" Margin="50,5" RenderTransformOrigin="0.5,0.5">
                            <Rectangle.RenderTransform>
                                <RotateTransform Angle="-40"/>
                            </Rectangle.RenderTransform>
                        </Rectangle>
                        <Rectangle Width="10" Height="100" Stroke="White" StrokeThickness="10" Margin="50,5" RenderTransformOrigin="0.5,0.5">
                            <Rectangle.RenderTransform>
                                <RotateTransform Angle="40"/>
                            </Rectangle.RenderTransform>
                        </Rectangle>
                    </Grid>
                </Border>
                <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Background="#b4b2b2" Width="1650" Height="800">
                    <ScrollViewer MaxHeight="800" MaxWidth="1650" Height="800" VerticalScrollBarVisibility="Hidden" Padding="10">
                        <StackPanel>
                            <TextBlock Text="FAQ" FontSize="30" FontWeight="ExtraBold" TextDecorations="Underline" HorizontalAlignment="Center" TextWrapping="WrapWithOverflow"></TextBlock>
                            <TextBlock Text="{Binding FaqText}" FontSize="30" FontWeight="DemiBold" HorizontalAlignment="Center" TextWrapping="WrapWithOverflow"></TextBlock>
                        </StackPanel>
                    </ScrollViewer>
                </StackPanel>
            </StackPanel>
            <!-- Endregion -->
            <!-- Region Sidebar -->
            <Border Grid.Row="2" Grid.RowSpan="1" Width="300" MaxWidth="300" HorizontalAlignment="Left" BorderBrush="White" BorderThickness="2" Margin="0,5,0,5" CornerRadius="5">
                <StackPanel Grid.Row="1" Grid.RowSpan="2" Width="200" MaxWidth="200" HorizontalAlignment="Center" Margin="10">
                    <Label Margin="10" Content="Menü" HorizontalAlignment="Center" FontSize="30" Foreground="White" FontWeight="DemiBold"/>
                    <Border BorderBrush="White" BorderThickness="1" CornerRadius="5" Padding="5">
                        <Button BorderBrush="Transparent" Content="Schließen" Background="Transparent" FontWeight="DemiBold" Foreground="White" FontSize="30" Command="{Binding btnCloseClickCommand}"></Button>
                    </Border>
                    <Border BorderBrush="White" BorderThickness="1" CornerRadius="5" Padding="5" Margin="0,5,0,0">
                        <Button BorderBrush="Transparent" Content="Logout" Background="Transparent" FontWeight="DemiBold" Foreground="White" FontSize="30" Command="{Binding btnLogoutClickCommand}"></Button>
                    </Border>
                    <Border BorderBrush="White" BorderThickness="1" CornerRadius="5" Padding="5" Margin="0,5,0,0">
                        <Button BorderBrush="Transparent" Content="FAQ" Background="Transparent" FontWeight="DemiBold" Foreground="White" FontSize="30" Command="{Binding btnShowFAQClickCommand}"></Button>
                    </Border>
                </StackPanel>
            </Border>
            <!-- Endregion -->
            <!--Region User Information-->
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Margin="10,4,0,0">
                <Label Content="Benutzernummer:" Foreground="White" FontWeight="DemiBold" FontSize="30"></Label>
                <Label Content="{Binding LoggedInUserID}" Foreground="White" FontWeight="DemiBold" FontSize="30"></Label>
            </StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="4">
                <Label Content="Guthaben" Foreground="White" FontWeight="DemiBold" FontSize="30"></Label>
                <Label Content="{Binding LoggedInUserMoney}" Foreground="White" FontWeight="DemiBold" FontSize="30" ContentStringFormat="{}{0} €"></Label>
            </StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="0,4,10,0">
                <Label Content="Benutzername:" Foreground="White" FontWeight="DemiBold" FontSize="30"></Label>
                <Label Content="{Binding LoggedInUsername}" Foreground="White" FontWeight="DemiBold" FontSize="30"></Label>
            </StackPanel>
            <!--Endregion-->
            <!--Region complete setted and usermoneyrange-->
            <StackPanel Grid.Row="1" Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="10">
                <StackPanel Orientation="Horizontal">
                    <Label Content="Wie viel möchten sie auf einmal setzen?" FontSize="30" Foreground="White" FontWeight="DemiBold"/>
                    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal">
                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                            <Button Content="▲" Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btnUserMoreMoneyRangeClickCommand}"></Button>
                        </Border>
                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                            <Label Content="{Binding UserMoneyRange}" Background="Transparent" Foreground="White" FontSize="30"></Label>
                        </Border>
                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                            <Button Content="▼" Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btnUserLessMoneyRangeClickCommand}"></Button>
                        </Border>
                    </StackPanel>
                </StackPanel>
                <StackPanel Orientation="Horizontal">
                    <StackPanel Orientation="Horizontal">
                        <Label Content="Gesamt gesetzt" Foreground="White" FontWeight="DemiBold" FontSize="30"></Label>
                        <Label Content="{Binding UserCompleteSettedMoney}" Foreground="White" FontWeight="DemiBold" FontSize="30"></Label>
                        <Label Content="€" Foreground="White" FontWeight="DemiBold" FontSize="30"/>
                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5" Padding="5" Margin="50,0,0,0">
                            <Button BorderBrush="Transparent" Content="Zurücksetzen" Background="Transparent" FontWeight="DemiBold" Foreground="White" FontSize="30" Command="{Binding btnResetUserSettedMoneyClickCommand}"></Button>
                        </Border>
                    </StackPanel>
                </StackPanel>
            </StackPanel>
            <!--Endregion-->
            <!--Region User selection-->
            <StackPanel Orientation="Horizontal" Grid.Row="2" HorizontalAlignment="Right">
                <StackPanel Margin="0,0,-180,0" VerticalAlignment="Center" HorizontalAlignment="Right">
                    <Border BorderBrush="White" BorderThickness="1" Width="460" Height="100" CornerRadius="5" Margin="5" RenderTransformOrigin="0.5,0.5" Background="Green">
                        <Border.RenderTransform>
                            <RotateTransform Angle="-90"/>
                        </Border.RenderTransform>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="60*"/>
                                <RowDefinition Height="40*"/>
                            </Grid.RowDefinitions>
                            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal">
                                <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                    <Button Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btn0MoreMoneyClickCommand}">
                                        <TextBlock Text="▲" RenderTransformOrigin="0.5,0.5">
                                            <TextBlock.RenderTransform>
                                                <RotateTransform Angle="90"/>
                                            </TextBlock.RenderTransform>
                                        </TextBlock>
                                    </Button>
                                </Border>
                                <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                    <Label Content="{Binding Number0MoneySet}" Background="Transparent" Foreground="White" FontSize="30" RenderTransformOrigin="0.5,0.5">
                                        <Label.RenderTransform>
                                            <RotateTransform Angle="90"/>
                                        </Label.RenderTransform>
                                    </Label>
                                </Border>
                                <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                    <Button Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btn0LessMoneyClickCommand}">
                                        <TextBlock Text="▼" RenderTransformOrigin="0.5,0.5">
                                            <TextBlock.RenderTransform>
                                                <RotateTransform Angle="90"/>
                                            </TextBlock.RenderTransform>
                                        </TextBlock>
                                    </Button>
                                </Border>
                            </StackPanel>
                            <StackPanel Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center">
                                <TextBlock Text="0" Foreground="White" FontWeight="DemiBold" FontSize="20" RenderTransformOrigin="0.5,0.5"></TextBlock>
                            </StackPanel>
                        </Grid>
                    </Border>
                </StackPanel>
                <StackPanel Grid.Row="2" HorizontalAlignment="Center" VerticalAlignment="Center">
                    <StackPanel Orientation="Horizontal">
                        <Border BorderBrush="White" BorderThickness="1" Width="411" Height="100" CornerRadius="5" Margin="5">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="60*"/>
                                    <RowDefinition Height="40*"/>
                                </Grid.RowDefinitions>
                                <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal">
                                    <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                        <Button Content="▲" Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btnLowNumbersMoreMoneyCommand}"></Button>
                                    </Border>
                                    <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                        <Label Content="{Binding LowNumbersMoneySet}" Background="Transparent" Foreground="White" FontSize="30"></Label>
                                    </Border>
                                    <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                        <Button Content="▼" Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btnLowNumbersLessMoneyCommand}"></Button>
                                    </Border>
                                </StackPanel>
                                <StackPanel Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center">
                                    <TextBlock Text="Niedrig 1-18" Foreground="White" FontWeight="DemiBold" FontSize="25"/>
                                </StackPanel>
                            </Grid>
                        </Border>
                        <Border BorderBrush="White" BorderThickness="1" Width="411" Height="100" CornerRadius="5" Margin="5">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="60*"/>
                                    <RowDefinition Height="40*"/>
                                </Grid.RowDefinitions>
                                <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal">
                                    <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                        <Button Content="▲" Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btnOddNumbersMoreMoneyClickCommand}"></Button>
                                    </Border>
                                    <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                        <Label Content="{Binding OddNumbersMoneySet}" Background="Transparent" Foreground="White" FontSize="30"></Label>
                                    </Border>
                                    <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                        <Button Content="▼" Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btnOddNumbersLessMoneyClickCommand}"></Button>
                                    </Border>
                                </StackPanel>
                                <StackPanel Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center">
                                    <TextBlock Text="Ungerade" Foreground="White" FontWeight="DemiBold" FontSize="25"/>
                                </StackPanel>
                            </Grid>
                        </Border>
                        <Border BorderBrush="White" BorderThickness="1" Width="411" Height="100" CornerRadius="5" Margin="5">
                            <Grid Margin="1,-1,-1,1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="60*"/>
                                    <RowDefinition Height="40*"/>
                                </Grid.RowDefinitions>
                                <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal">
                                    <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                        <Button Content="▲" Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btnAllRedNumbersMoreMoneyCommand}"></Button>
                                    </Border>
                                    <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                        <Label Content="{Binding AllRedNumbersMoneySet}" Background="Transparent" Foreground="White" FontSize="30"></Label>
                                    </Border>
                                    <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                        <Button Content="▼" Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btnAllRedNumbersLessMoneyCommand}"></Button>
                                    </Border>
                                </StackPanel>
                                <StackPanel Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center">
                                    <TextBlock Text="Rot" Foreground="White" FontWeight="DemiBold" FontSize="25"/>
                                </StackPanel>
                            </Grid>
                        </Border>
                    </StackPanel>
                    <StackPanel>
                        <StackPanel Orientation="Horizontal" Margin="-25,20,0,0">
                            <Border BorderBrush="White" BorderThickness="1" Width="150" Height="100" CornerRadius="5" Margin="5" RenderTransformOrigin="0.5,0.5" Background="Red">
                                <Border.RenderTransform>
                                    <RotateTransform Angle="-90"/>
                                </Border.RenderTransform>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="60*"/>
                                        <RowDefinition Height="40*"/>
                                    </Grid.RowDefinitions>
                                    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal">
                                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                            <Button Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btn3MoreMoneyClickCommand}">
                                                <TextBlock Text="▲" RenderTransformOrigin="0.5,0.5">
                                                    <TextBlock.RenderTransform>
                                                        <RotateTransform Angle="90"/>
                                                    </TextBlock.RenderTransform>
                                                </TextBlock>
                                            </Button>
                                        </Border>
                                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                            <Label Content="{Binding Number3MoneySet}" Background="Transparent" Foreground="White" FontSize="30" RenderTransformOrigin="0.5,0.5">
                                                <Label.RenderTransform>
                                                    <RotateTransform Angle="90"/>
                                                </Label.RenderTransform>
                                            </Label>
                                        </Border>
                                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                            <Button Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btn3LessMoneyClickCommand}">
                                                <TextBlock Text="▼" RenderTransformOrigin="0.5,0.5">
                                                    <TextBlock.RenderTransform>
                                                        <RotateTransform Angle="90"/>
                                                    </TextBlock.RenderTransform>
                                                </TextBlock>
                                            </Button>
                                        </Border>
                                    </StackPanel>
                                    <StackPanel Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <TextBlock Text="3" Foreground="White" FontWeight="DemiBold" FontSize="20" RenderTransformOrigin="0.5,0.5"></TextBlock>
                                    </StackPanel>
                                </Grid>
                            </Border>
                            <Border BorderBrush="White" BorderThickness="1" Width="150" Height="100" CornerRadius="5" Margin="-50,0,0,0" RenderTransformOrigin="0.5,0.5" Background="Black">
                                <Border.RenderTransform>
                                    <RotateTransform Angle="-90"/>
                                </Border.RenderTransform>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="60*"/>
                                        <RowDefinition Height="40*"/>
                                    </Grid.RowDefinitions>
                                    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal">
                                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                            <Button Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btn6MoreMoneyClickCommand}">
                                                <TextBlock Text="▲" RenderTransformOrigin="0.5,0.5">
                                                    <TextBlock.RenderTransform>
                                                        <RotateTransform Angle="90"/>
                                                    </TextBlock.RenderTransform>
                                                </TextBlock>
                                            </Button>
                                        </Border>
                                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                            <Label Content="{Binding Number6MoneySet}" Background="Transparent" Foreground="White" FontSize="30" RenderTransformOrigin="0.5,0.5">
                                                <Label.RenderTransform>
                                                    <RotateTransform Angle="90"/>
                                                </Label.RenderTransform>
                                            </Label>
                                        </Border>
                                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                            <Button Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btn6LessMoneyClickCommand}">
                                                <TextBlock Text="▼" RenderTransformOrigin="0.5,0.5">
                                                    <TextBlock.RenderTransform>
                                                        <RotateTransform Angle="90"/>
                                                    </TextBlock.RenderTransform>
                                                </TextBlock>
                                            </Button>
                                        </Border>
                                    </StackPanel>
                                    <StackPanel Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <TextBlock Text="6" Foreground="White" FontWeight="DemiBold" FontSize="20" RenderTransformOrigin="0.5,0.5"></TextBlock>
                                    </StackPanel>
                                </Grid>
                            </Border>
                            <Border BorderBrush="White" BorderThickness="1" Width="150" Height="100" CornerRadius="5" Margin="-45,0,0,0" RenderTransformOrigin="0.5,0.5" Background="Red">
                                <Border.RenderTransform>
                                    <RotateTransform Angle="-90"/>
                                </Border.RenderTransform>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="60*"/>
                                        <RowDefinition Height="40*"/>
                                    </Grid.RowDefinitions>
                                    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal">
                                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                            <Button Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btn9MoreMoneyClickCommand}">
                                                <TextBlock Text="▲" RenderTransformOrigin="0.5,0.5">
                                                    <TextBlock.RenderTransform>
                                                        <RotateTransform Angle="90"/>
                                                    </TextBlock.RenderTransform>
                                                </TextBlock>
                                            </Button>
                                        </Border>
                                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                            <Label Content="{Binding Number9MoneySet}" Background="Transparent" Foreground="White" FontSize="30" RenderTransformOrigin="0.5,0.5">
                                                <Label.RenderTransform>
                                                    <RotateTransform Angle="90"/>
                                                </Label.RenderTransform>
                                            </Label>
                                        </Border>
                                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                            <Button Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btn9LessMoneyClickCommand}">
                                                <TextBlock Text="▼" RenderTransformOrigin="0.5,0.5">
                                                    <TextBlock.RenderTransform>
                                                        <RotateTransform Angle="90"/>
                                                    </TextBlock.RenderTransform>
                                                </TextBlock>
                                            </Button>
                                        </Border>
                                    </StackPanel>
                                    <StackPanel Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <TextBlock Text="9" Foreground="White" FontWeight="DemiBold" FontSize="20" RenderTransformOrigin="0.5,0.5"></TextBlock>
                                    </StackPanel>
                                </Grid>
                            </Border>
                            <Border BorderBrush="White" BorderThickness="1" Width="150" Height="100" CornerRadius="5" Margin="-45,0,0,0" RenderTransformOrigin="0.5,0.5" Background="Black">
                                <Border.RenderTransform>
                                    <RotateTransform Angle="-90"/>
                                </Border.RenderTransform>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="60*"/>
                                        <RowDefinition Height="40*"/>
                                    </Grid.RowDefinitions>
                                    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal">
                                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                            <Button Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btn12MoreMoneyClickCommand}">
                                                <TextBlock Text="▲" RenderTransformOrigin="0.5,0.5">
                                                    <TextBlock.RenderTransform>
                                                        <RotateTransform Angle="90"/>
                                                    </TextBlock.RenderTransform>
                                                </TextBlock>
                                            </Button>
                                        </Border>
                                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                            <Label Content="{Binding Number12MoneySet}" Background="Transparent" Foreground="White" FontSize="30" RenderTransformOrigin="0.5,0.5">
                                                <Label.RenderTransform>
                                                    <RotateTransform Angle="90"/>
                                                </Label.RenderTransform>
                                            </Label>
                                        </Border>
                                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                            <Button Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btn12LessMoneyClickCommand}">
                                                <TextBlock Text="▼" RenderTransformOrigin="0.5,0.5">
                                                    <TextBlock.RenderTransform>
                                                        <RotateTransform Angle="90"/>
                                                    </TextBlock.RenderTransform>
                                                </TextBlock>
                                            </Button>
                                        </Border>
                                    </StackPanel>
                                    <StackPanel Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <TextBlock Text="12" Foreground="White" FontWeight="DemiBold" FontSize="20" RenderTransformOrigin="0.5,0.5"></TextBlock>
                                    </StackPanel>
                                </Grid>
                            </Border>
                            <Border BorderBrush="White" BorderThickness="1" Width="150" Height="100" CornerRadius="5" Margin="-45,0,0,0" RenderTransformOrigin="0.5,0.5" Background="Red">
                                <Border.RenderTransform>
                                    <RotateTransform Angle="-90"/>
                                </Border.RenderTransform>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="60*"/>
                                        <RowDefinition Height="40*"/>
                                    </Grid.RowDefinitions>
                                    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal">
                                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                            <Button Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btn15MoreMoneyClickCommand}">
                                                <TextBlock Text="▲" RenderTransformOrigin="0.5,0.5">
                                                    <TextBlock.RenderTransform>
                                                        <RotateTransform Angle="90"/>
                                                    </TextBlock.RenderTransform>
                                                </TextBlock>
                                            </Button>
                                        </Border>
                                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                            <Label Content="{Binding Number15MoneySet}" Background="Transparent" Foreground="White" FontSize="30" RenderTransformOrigin="0.5,0.5">
                                                <Label.RenderTransform>
                                                    <RotateTransform Angle="90"/>
                                                </Label.RenderTransform>
                                            </Label>
                                        </Border>
                                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                            <Button Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btn15LessMoneyClickCommand}">
                                                <TextBlock Text="▼" RenderTransformOrigin="0.5,0.5">
                                                    <TextBlock.RenderTransform>
                                                        <RotateTransform Angle="90"/>
                                                    </TextBlock.RenderTransform>
                                                </TextBlock>
                                            </Button>
                                        </Border>
                                    </StackPanel>
                                    <StackPanel Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <TextBlock Text="15" Foreground="White" FontWeight="DemiBold" FontSize="20" RenderTransformOrigin="0.5,0.5"></TextBlock>
                                    </StackPanel>
                                </Grid>
                            </Border>
                            <Border BorderBrush="White" BorderThickness="1" Width="150" Height="100" CornerRadius="5" Margin="-45,0,0,0" RenderTransformOrigin="0.5,0.5" Background="Black">
                                <Border.RenderTransform>
                                    <RotateTransform Angle="-90"/>
                                </Border.RenderTransform>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="60*"/>
                                        <RowDefinition Height="40*"/>
                                    </Grid.RowDefinitions>
                                    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal">
                                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                            <Button Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btn18MoreMoneyClickCommand}">
                                                <TextBlock Text="▲" RenderTransformOrigin="0.5,0.5">
                                                    <TextBlock.RenderTransform>
                                                        <RotateTransform Angle="90"/>
                                                    </TextBlock.RenderTransform>
                                                </TextBlock>
                                            </Button>
                                        </Border>
                                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                            <Label Content="{Binding Number18MoneySet}" Background="Transparent" Foreground="White" FontSize="30" RenderTransformOrigin="0.5,0.5">
                                                <Label.RenderTransform>
                                                    <RotateTransform Angle="90"/>
                                                </Label.RenderTransform>
                                            </Label>
                                        </Border>
                                        <Border BorderBrush="White" BorderThickness="1" CornerRadius="5">
                                            <Button Background="Transparent" Foreground="White" FontSize="30" Command="{Binding btn18LessMoneyClickCommand}">
                                                <TextBlock Text="▼" RenderTransformOrigin="0.5,0.5">
                                                    <TextBlock.RenderTransform>
                                                        <RotateTransform Angle="90"/>
                                                    </TextBlock.RenderTransform>
                                                </TextBlock>
                                            </Button>
                                        </Border>
